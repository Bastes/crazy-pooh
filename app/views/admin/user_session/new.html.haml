= form_for @user_session, :url => admin_user_session_path(@user_session) do |f|
  - if @user_session.errors.any?
    %div#error= @user_session.errors.full_messages.first
  %div
    = f.label :login
    = f.text_field :login
  %div
    = f.label :password
    = f.password_field :password
  %div
    = f.submit
